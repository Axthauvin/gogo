{
  "manifest_version": 2,
  "name": "Gogo - Local URL Shortener",
  "version": "1.0",
  "description": "Local url shortener to go faster on the internet.",
  "permissions": ["tabs", "webNavigation", "storage", "<all_urls>"],
  "background": {
    "scripts": ["src/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "src/popup.html",
    "default_title": "Gogo - Local URL Shortener"
  },
  "omnibox": {
    "keyword": "go"
  },
  "options_page": "src/settings.html",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "icons": {
    "16": "src/icons/favicon-16x16.png",
    "48": "src/icons/favicon-48x48.png",
    "128": "src/icons/favicon-128x128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{49535a80-9542-4a02-856c-4fb572c30f35}"
    }
  }
}
