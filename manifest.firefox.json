{
  "manifest_version": 2,
  "name": "GoGo â€“ Your Shortcut Engine for the Web",
  "author": "Axel Thauvin",
  "version": "1.0",
  "description": "Create custom shortcuts for your favorite sites. Jump anywhere instantly, with all data stored locally.",
  "permissions": ["tabs", "webNavigation", "storage", "<all_urls>"],
  "background": {
    "scripts": ["src/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "src/popup.html",
    "default_title": "Gogo - Local URL Shortener"
  },
  "omnibox": {
    "keyword": "go"
  },
  "options_page": "src/settings.html",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "icons": {
    "16": "src/icons/favicon-16x16.png",
    "48": "src/icons/favicon-48x48.png",
    "128": "src/icons/favicon-128x128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{49535a80-9542-4a02-856c-4fb572c30f35}",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  },
  "license": "MIT"
}
